<%= form_for(@degree_type) do |f| %>

  <%= render 'shared/error_messages', object: @degree_type %>

  <div class="form-group">
    <%= f.label :degree_type_name %>
    <%= f.text_field :degree_type_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.label :degree_type_notes %>
    <%= f.text_area :degree_type_notes, class: "form-control" %>
  </div>

  <%= f.submit class: "btn btn-default" %>

<% end %>
