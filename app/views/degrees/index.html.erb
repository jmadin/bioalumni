<h1>Listing degrees</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Alum</th>
      <th>Degree name</th>
      <th>Graduation year</th>
      <th>Thesis title</th>
      <th>Degree notes</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @degrees.each do |degree| %>
      <tr>
        <td><%= degree.user %></td>
        <td><%= degree.alum %></td>
        <td><%= degree.degree_name %></td>
        <td><%= degree.graduation_year %></td>
        <td><%= degree.thesis_title %></td>
        <td><%= degree.degree_notes %></td>
        <td><%= link_to 'Show', degree %></td>
        <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
        <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Degree', new_degree_path %>
